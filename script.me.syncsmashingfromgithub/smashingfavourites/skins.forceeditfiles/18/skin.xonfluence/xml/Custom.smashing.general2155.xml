				<bordersize>12</bordersize>
			</control>
			<control type="list" id="450">
				<left>485r</left>	<!-- was 585r -->
				<top>250</top>		<!-- Default 375 - reduce and list moves up-->
				<width>575</width>	<!-- was 675 -->
				<height>700</height>	<!-- Default is 570 - reduce and  see less entries, increase see more-->
				<onup>450</onup>
				<onleft>Dialog.Close(2155)</onleft>
				<onright>450</onright>
				<ondown>450</ondown>
				<pagecontrol></pagecontrol>
				<scrolltime>200</scrolltime>
				<itemlayout height="60">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>555</width>
						<height>60</height>
						<texture border="8">smashingbutton-nofocus.png</texture>
					</control>
					<control type="label">
						<left>15</left>
						<top>0</top>
						<width>525</width>
						<height>60</height>
						<font>font12_title</font>
						<align>left</align>
						<aligny>center</aligny>
						<selectedcolor>selected</selectedcolor>
						<info>ListItem.Label</info>
					</control>
				</itemlayout>
				<focusedlayout height="60">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>555</width>
						<height>60</height>
						<visible>!Control.HasFocus(450)</visible>
						<texture border="4">smashingbutton-nofocus.png</texture>
					</control>
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>555</width>
						<height>60</height>
						<visible>Control.HasFocus(450)</visible>
